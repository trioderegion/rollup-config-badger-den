{
  // Test
  "version": "1.0.0",
  "title": "Badger Den Demo Module",
  "description": "Demonstration module built from the Badger Den",
  "projectUrl": "https://www.npmjs.com/rollup-config-badger-den",
  "entryPoints": {
    "lang": "static/lang/**",
    "templates": "templates/*",
    "external": "lib/tiny.mjs",
    "compendia": {
      "path": "packs/*/",
      "type": {
        "demo-actors": "Actor"
      },
      "label": {
        "demo-actors": "Demo Actors"
      },
      "banner": "static/assets/banner.webp",
      "folder": {
        "label": "Demo Module Content",
        "color": "#FF0000"
      },
      "system": "nova"
    }
  },
  "package": {
    "name":"%config.id%-%config.version%",
    "protected": false,
    "manifest": "",
    "download": ""
  },
  "dest":"../built-demo-module",
  "profile": {
    "altered": {
      "id": "altered",
      "version": "2.0.0",
      "package": {
        "create": true,
        "protected": false,
        "manifest": "https://github.com/trioderegion/rollup-config-badger-den/releases/download/%config.version%/module.json",
        "download": "https://github.com/trioderegion/rollup-config-badger-den/releases/download/%config.version%/%config.package.name%.zip"
      },
      "dest": "../built-altered-demo",
      "entryPoints": {
        "main": "index.mjs"
      },
      "compress":false,
      "sourcemaps": true,
      "clean": true,
      "hmr": true,
      "flags": {
        "dnd5e": {
          "test": true
        }
      }
    },
    "test":{
      "entryPoints": {
        "main": ["index.mjs", "dev/utility-module.mjs"]
      },
      "compress":false,
      "sourcemaps": true,
      "clean": true,
      "hmr": true,
      "static": ["dev/html"],
      "package": {
        "create": true,
        "protected": false,
        "manifest": "https://github.com/trioderegion/rollup-config-badger-den/releases/download/%config.version%/module.json",
        "download": "https://github.com/trioderegion/rollup-config-badger-den/releases/download/%config.version%/%config.package.name%.zip"
      }
    },
    "css": {
      "clean": true,
      "id": "demo-module-css",
      "entryPoints": {
        "main": "index-css.mjs"
      }
    },
    "scss": {
      "clean": true,
      "id": "demo-module-scss",
      "entryPoints": {
        "main": "index-scss.mjs"
      }
    },
    "less": {
      "clean": true,
      "id": "demo-module-less",
      "entryPoints": {
        "main": "index-less.mjs"
      }
    }
  },
  "dependencies": {
    "core": [
      "10",
      "11.299",
      "12"
    ]
  },
  "storage": true,
  "static": [
    "static"
  ],
  "authors": [
    {
      "name": "trioderegion",
      "url": "https://www.github.com/trioderegion"
    }
  ]
}
